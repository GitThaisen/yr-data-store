{
  "name": "@yr/data-store",
  "description": "A clever data object",
  "version": "1.0.0",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
    "@yr/agent": "4.2.x",
    "@yr/clock": "1.1.x",
    "@yr/keys": "1.1.x",
    "@yr/property": "5.0.x",
    "@yr/runtime": "1.2.x",
    "@yr/time": "3.2.x",
    "debug": "2.2.0",
    "eventemitter3": "1.2.0",
    "is-plain-obj": "1.1.0",
    "object-assign": "4.1.0",
    "uuid": "2.0.2"
  },
  "devDependencies": {
    "buddy": "5.1.x",
    "buddy-plugin-babel": "6.10.x",
    "expect.js": "*",
    "mocha": "*",
    "mocha-phantomjs": "*",
    "nock": "*"
  },
  "main": "src/index.js",
  "repository": "https://github.com/YR/data-store.git",
  "license": "MIT",
  "scripts": {
    "prepublish": "buddy build",
    "test": "NODE_ENV=test mocha test/lib-test.js --reporter spec",
    "test-client": "buddy build && mocha-phantomjs test/test-runner.html"
  },
  "browser": "index.js",
  "build": {
    "targets": [
      {
        "input": "src",
        "output": ".",
        "modular": false
      },
      {
        "input": "src/index.js",
        "output": "test/lib.js"
      }
    ]
  }
}
